{
    "$schema": "https://github.com/ManualForArchipelago/Manual/raw/main/schemas/Manual.regions.schema.json",
    "In_the_Wall":{
        "connects_to": ["Dead_City"],
        "starting": true
    },
    "Dead_City": {
        "connects_to": ["The_Flat"],
        "exit_requires": {
            "The_Flat": "|Bucket| AND |Battery:1|"
        }
    },
    "The_Flat": {
        "connects_to": ["Slums"],
        "exit_requires": {
            "Slums": "|Postcard| AND |Keys|"
        }
    },
    "Slums": {
        "connects_to": ["Roofs","Dead_Tracks"],
        "exit_requires": {
            "Roofs": "|@Notebooks:4| AND |Safe Keys| AND |Sender|",
            "Dead_Tracks": "|Tracker| AND |@Notebooks:4| AND |Safe Keys| AND |Sender| AND |Poncho| AND |Electric cables|"
        }
    },
    "Roofs": {
        "connects_to": ["Slums"]
    },
    "Dead_Tracks": {
        "connects_to": ["The_Sewers"],
        "exit_requires": {
            "The_Sewers": "|Defluxor| AND |Fuse|"
        }
    },
    "The_Sewers": {
        "connects_to": ["Antvillage"]
    },
    "Antvillage": {
        "connects_to": ["Middeltown"]
    },
    "Middeltown": {
        "connects_to": ["Jail"],
        "exit_requires": {
            "Jail": "|Music cassette| AND |Hat| AND |Jacket| AND |Note for contactperson| AND |Atomic Battery|"
        }
    },
    "Jail": {
        "connects_to": ["Opperations_Room"],
        "exit_requires": {
            "Opperations_Room": "|Atomic Battery| AND |Metro Keys|"
        }
    },
    "Opperations_Room": {
        "connects_to": []
    }
}
